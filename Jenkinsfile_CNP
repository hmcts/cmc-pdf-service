#!groovy

@Library("Infrastructure@docker-test-vm-config")

def product = "cmc"
def component = "pdf-service"

withPipeline("java", product, component) {
  enableSlackNotifications('#rpe-build-notices')
  enableDockerBuild()
  enableAksStagingDeployment()
  disableLegacyDeployment()
  installCharts()
  
}
